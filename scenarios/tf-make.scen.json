{
    "name": "Adding NFT",
    "steps": [
        {
            "step": "externalSteps",
            "path": "tf-init.scen.json"
        },
        {
            "step": "setState",
            "currentBlockInfo": {
                "blockTimestamp": "200,000"
            }
        },
        {
            "step": "scCall",
            "id": "make",
            "tx": {
                "from": "address:owner",
                "to": "sc:tokemonworld",
                "function": "make_nft",
                "arguments": [
                    "str:NFT1",
                    "str:https://unsplash.com/fr/photos/un-jouet-orange-pose-sur-un-sol-en-ciment-WC_Qjaryv4Y"
                ],
                "gasLimit": "20,000,000",
                "gasPrice": "0"
            },
            "expect": {
                "out": [],
                "status": "0",
                "message": "",
                "gas": "*",
                "refund": "*"
            }
        },

        {
            "step": "checkState",
            "accounts": {
                "address:owner": {
                    "nonce": "2",
                    "balance": "99999"
                }
            }
        }
    ]
}
